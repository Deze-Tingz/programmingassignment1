package playground;

import java.util.Scanner;

public class SimpleWhileLoop {
	public static void main(String[] args) {
		Scanner scnr = new Scanner(System.in);


		int inputValue;
		int smallestVal = 0;
		int i;
		inputValue = scnr.nextInt();

		for (i = 0; i < 6; ++i) {
			// This will only run once
			if (i == 0) {
				smallestVal = inputValue;

			}

			else if (smallestVal < inputValue) {

				smallestVal = inputValue;

			}
			inputValue = scnr.nextInt();

		}
		System.out.println(smallestVal);

	}

}

