package playground;

import java.util.Scanner;

public class PLayGround {

	public static void main(String[] args) {
		Scanner scnr = new Scanner(System.in);
	      int userIn;

			userIn = scnr.nextInt();

	      while (userIn <= 9) {
	    	  
	    	  System.out.println(userIn);
	    	  userIn = userIn + 3;
	      }


	}
}


